{
  "name": "stable-tor-api",
  "version": "1.0.0",
  "description": "(SMLD) Stable Magnet Link Downloader",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "tsc -w",
    "watch-dev": "env-cmd --silent -f .env nodemon --inspect --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "tsc",
    "precommit": "lint-staged",
    "start": "env-cmd --silent -f .env node dist/index.js",
    "typeorm": "env-cmd --silent -f .env typeorm-ts-node-esm",
    "typeorm:migration-up": "npm run typeorm migration:run -- -d ./src/typeorm.config.ts",
    "typeorm:migration-down": "npm run typeorm migration:revert -- -d ./src/typeorm.config.ts",
    "typeorm:migration-generate": "npm run typeorm -- -d ./src/typeorm.config.ts migration:generate ./src/migration/$FOR",
    "typeorm:migration-up-fake": "npm run typeorm -- -d ./src/typeorm.config.ts migration:run --fake",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migration/$FOR",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/typeorm.config.ts migration:revert",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "git@elitale.com",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/express-correlation-id": "^1.2.6",
    "@types/http-graceful-shutdown": "^2.3.0",
    "@types/ioredis": "^5.0.0",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/p-queue": "^3.2.1",
    "@types/parse-torrent": "^5.8.7",
    "@types/request-ip": "^0.0.41",
    "@types/webtorrent": "^0.109.7",
    "@types/wtfnode": "^0.7.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "conventional-changelog-atom": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/lib-storage": "^3.495.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "@bull-board/api": "^5.13.0",
    "@bull-board/express": "^5.13.0",
    "@bull-board/ui": "^5.13.0",
    "@ctrl/magnet-link": "^4.0.1",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "bullmq": "^5.1.4",
    "cheerio": "^1.0.0-rc.12",
    "common-stuff": "^1.10.3",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "express-correlation-id": "^2.0.1",
    "fetch-ponyfill": "^7.1.0",
    "firebase-admin": "^12.0.0",
    "http-graceful-shutdown": "^3.1.13",
    "ioredis": "^5.3.2",
    "isomorphic-htmlparser": "^1.3.0",
    "multer": "^1.4.5-lts.1",
    "p-queue": "^8.0.1",
    "parse-torrent": "^11.0.14",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "request-ip": "^3.3.0",
    "typeorm": "^0.3.19",
    "utp-native": "^2.5.3",
    "webtorrent": "^2.1.33",
    "winston": "^3.11.0",
    "wtfnode": "^0.9.1"
  }
}
