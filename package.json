{
  "name": "stable-tor-api",
  "version": "1.0.0",
  "description": "(SMLD) Stable Magnet Link Downloader",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "dev": "PORT=3000 env-cmd -f .env tsx watch --inspect ./src/index.ts",
    "start": "node dist/index.js",
    "typeorm": "env-cmd -f .env typeorm-ts-node-esm",
    "typeorm:migration-up": "npm run typeorm migration:run -- -d ./src/typeorm.config.ts",
    "typeorm:migration-down": "npm run typeorm migration:revert -- -d ./src/typeorm.config.ts",
    "typeorm:migration-generate": "npm run typeorm -- -d ./src/typeorm.config.ts migration:generate ./src/migration/$FOR",
    "typeorm:migration-up-fake": "npm run typeorm -- -d ./src/typeorm.config.ts migration:run --fake",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migration/$FOR",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/typeorm.config.ts migration:revert"
  },
  "keywords": [],
  "author": "git@elitale.com",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/express-correlation-id": "^1.2.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/p-queue": "^3.2.1",
    "@types/parse-torrent": "^5.8.7",
    "@types/request-ip": "^0.0.41",
    "@types/webtorrent": "^0.109.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.2",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-extensions": "^0.1.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "express-correlation-id": "^2.0.1",
    "multer": "^1.4.5-lts.1",
    "p-queue": "^8.0.1",
    "parse-torrent": "^11.0.14",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "request-ip": "^3.3.0",
    "typeorm": "^0.3.19",
    "webtorrent": "^2.1.33",
    "winston": "^3.11.0"
  }
}
